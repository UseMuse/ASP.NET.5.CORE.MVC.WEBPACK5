@{
    ViewData["Title"] = "DataTables";
}


@section Scripts
{
    <link href="~/dist/css/Home.DataTables.css" rel="stylesheet" />
    <script src="~/dist/Home.DataTables.js"></script>
    <script>
        async function ready() {
            /*   $('#example').DataTable();*/
            await renderTable()
        }
        document.addEventListener("DOMContentLoaded", ready);

    </script>
}
<h2>@ViewData["Title"]</h2>
<h3>@ViewData["Message"]</h3>
<table id="example" class="table table-striped table-bordered" style="width:100%">
    <thead>
        <tr>
            <th>image</th>
            <th>publishDate</th>
            <th>text</th>
            <th>tags</th>
            <th>link</th>
            <th>likes</th>
        </tr>
    </thead>
    <tfoot>
        <tr>
            <th>image</th>
            <th>publishDate</th>
            <th>text</th>
            <th>tags</th>
            <th>link</th>
            <th>likes</th>
        </tr>
    </tfoot>
</table>